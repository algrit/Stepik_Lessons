https://stepik.org/lesson/24461/step/8?unit=6767 

class MoneyBox:
    def __init__(self, capacity):
        self.capacity = capacity
        self.coins = 0


    def can_add(self, v):
        if self.coins + v <= self.capacity:
            return True
        else:
            return False


    def add(self, v):
        if self.can_add(v) == True:
            self.coins += v

x = MoneyBox(9)
x.add(5)
x.can_add(2)
x.add(9)
